             xmlns:cp='urn:import:com.databasepreservation.common.client.common.visualization.browse.configuration'>

    <g:FlowPanel>
        <cp:ConfigurationStatusPanel ui:field="configurationStatusPanel"/>
        <g:FlowPanel ui:field="content" addStyleNames="management">
            <g:FlowPanel addStyleNames="structure-header" ui:field="mainHeader">
                <g:FlowPanel addStyleNames="data-transformation-toolbar-actions">
